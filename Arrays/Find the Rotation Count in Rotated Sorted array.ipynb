    "def countRotations(arr, n):\n",
    " \n",
    " \n",
    "    min = arr[0]\n",
    "    for i in range(0, n):\n",
    "     \n",
    "        if (min > arr[i]):\n",
    "         \n",
    "            min = arr[i]\n",
    "            min_index = i\n",
    "         \n",
    "    return min_index;\n",
    " \n",
    " \n",
    "\n",
    "arr = [15, 18, 2, 3, 6, 12]\n",
    "n = len(arr)\n",
    "print(countRotations(arr, n))"
   ]
